import random

letters = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ"
numbers = "1234567890"
symbols = "/?+-:!@#%"
lenght = 12
password = ""

for i in range(6):
    password += random.choice(letters)


for i in range(4):
    password += random.choice(numbers)


for i in range(2):
    password += random.choice(symbols)
     
print(password)
